[Desktop Entry]
Type=Application
X-Nemo-Application-Type=silica-qt5
Comment=Telegram client
Icon=depecher
Name=Depecher
#https://git.merproject.org/mer-core/libcontentaction/blob/master/src/doc.h
X-Maemo-Service=org.blacksailer.depecher
X-Maemo-Object-Path=/org/blacksailer/depecher
X-Maemo-Method=org.blacksailer.depecher.ShowApp
X-Maemo-Fixed-Args=Start
Exec=/usr/bin/invoker  -s --type=silica-qt5 /usr/bin/depecher